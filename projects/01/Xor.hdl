/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=anot);
    Not(in=b, out=bnot);
    And(a=a, b=bnot, out=abnot);
    And(a=anot, b=b, out=anotb);
    Or(a=abnot, b=anotb, out=out);
}
